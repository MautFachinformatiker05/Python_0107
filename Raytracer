a =[
    "                   ",
    "  xxxxx  xxxxx xxx ",
    " x       x      x  ",
    " x       x      x  ",
    " x  xxx  xxxxx  x  ",
    " x    x  x      x  ",
    " x    x  x      x  ",
    "  xxxx   x     xxxx",
    "                   "
]

ergebnis=[]
for d in a:
    print(d)
    wert=0
    faktor=1

    for e in range((len(d)-1),0,-1):
        if d[e]=="x":
            wert +=faktor
        faktor *=2
    ergebnis.append(wert)

print(ergebnis)

